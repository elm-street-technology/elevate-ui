{
  "description": "A component used to render a styled hr.",
  "displayName": "Hr",
  "methods": [],
  "props": {
    "className": {
      "flowType": {
        "name": "string"
      },
      "required": true,
      "description": "Accepts any classNames to be passed to the component."
    },
    "classes": {
      "flowType": {
        "name": "Object"
      },
      "required": true,
      "description": ""
    },
    "color": {
      "flowType": {
        "name": "string"
      },
      "required": false,
      "description": "Color to be applied to the Hr component. Can be \"primary\", \"secondary\", or any custom value."
    },
    "gutterBottom": {
      "flowType": {
        "name": "boolean"
      },
      "required": false,
      "description": "Adds default margin bottom",
      "defaultValue": {
        "value": "true",
        "computed": false
      }
    },
    "gutterTop": {
      "flowType": {
        "name": "boolean"
      },
      "required": false,
      "description": "Adds default margin top",
      "defaultValue": {
        "value": "true",
        "computed": false
      }
    },
    "style": {
      "flowType": {
        "name": "Object"
      },
      "required": true,
      "description": "Style object with any applicable overrides"
    },
    "theme": {
      "flowType": {
        "name": "Object"
      },
      "required": true,
      "description": "Default theme object from `withStyles`"
    },
    "thickness": {
      "flowType": {
        "name": "union",
        "raw": "string | number",
        "elements": [
          {
            "name": "string"
          },
          {
            "name": "number"
          }
        ]
      },
      "required": false,
      "description": "Height/thickness of the Hr component."
    }
  }
}