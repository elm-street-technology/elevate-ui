{
  "description": "A component used to render a styled hr.",
  "displayName": "Hr",
  "methods": [],
  "props": {
    "className": {
      "flowType": {
        "name": "string"
      },
      "required": true,
      "description": "Accepts any classNames to be passed to the component."
    },
    "color": {
      "flowType": {
        "name": "string"
      },
      "required": true,
      "description": "Color to be applied to the Hr component. Can be \"primary\", \"secondary\", or any custom value."
    },
    "style": {
      "flowType": {
        "name": "Object"
      },
      "required": true,
      "description": "Style object with any applicable overrides (common overrides are marginTop/marginBottom)"
    },
    "theme": {
      "flowType": {
        "name": "Object"
      },
      "required": true,
      "description": "Default theme object from `withStyles`"
    },
    "thickness": {
      "flowType": {
        "name": "union",
        "raw": "string | number",
        "elements": [
          {
            "name": "string"
          },
          {
            "name": "number"
          }
        ]
      },
      "required": true,
      "description": "Height/thickness of the Hr component."
    }
  }
}