  "peerDependencies": {
    "react": ">16.0.0",
    "react-dom": ">16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.11.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.4.0",
    "formik": "0.11.11",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "nwb": "0.21.5",
    "prettier": "1.11.1",
    "react": "16.2.0",
