{
  "name": "elevate-ui",
  "version": "0.0.9",
  "description": "An opinionated toolkit of customizable, easy-to-use React components for building scalable, maintainable web applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
